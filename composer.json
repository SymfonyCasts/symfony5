{
    "type": "project",
    "license": "proprietary",
    "name": "symfonycasts/symfony6_upgrade",
    "description": "Upgrade to Symfony 6 course code",
    "require": {
        "php": "^7.4.0 || ^8.0.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": "^2.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.13",
        "doctrine/dbal": "^2.13",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.0",
        "knplabs/knp-markdown-bundle": "^1.8",
        "knplabs/knp-time-bundle": "^1.18",
        "sensio/framework-extra-bundle": "^6.0",
        "sentry/sentry-symfony": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/asset": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/property-access": "5.0.*",
        "symfony/property-info": "5.0.*",
        "symfony/proxy-manager-bridge": "5.0.*",
        "symfony/routing": "5.1.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer": "5.0.*",
        "symfony/stopwatch": "5.0.*",
        "symfony/twig-bundle": "5.0.*",
        "symfony/ux-chartjs": "^1.0",
        "symfony/validator": "5.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.0.*",
        "symfonycasts/verify-email-bundle": "^1.7",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.3",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "symfony/debug-bundle": "5.0.*",
        "symfony/maker-bundle": "^1.15",
        "symfony/var-dumper": "5.0.*",
        "symfony/web-profiler-bundle": "5.0.*",
        "zenstruck/foundry": "^1.16"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
